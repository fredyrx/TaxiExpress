  
   <h1>Sign up</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@pasajero) do |f| %>
      <%= f.label :nombre_pasajero %>
      <%= f.text_field :nombre_pasajero %>

      <%= f.label :correo %>
      <%= f.email_field :correo %>

    

      <%= f.submit "Create my account", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>